<odoo>
    <record id="vegetarian_types_form" model="ir.ui.view">
        <field name="name">vegetarian_types.form</field>
        <field name="model">medical.vegetarian_types</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <label for="name"/>
                        <field name="name"/>
                        <label for="code"/>
                        <field name="code"/>
                        <newline/>
                        <label for="desc"/>
                        <field name="desc"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
